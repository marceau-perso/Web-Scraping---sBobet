package MODEL;

import java.util.Vector;

public class MatchManager {
	
	private Vector<Match> matches = null;
	public MatchManager(){
		matches = new Vector<Match>();
	}
	public void addMatch(String nom, String link){
		matches.add(new Match(nom, link));
	}
	
	public Vector<Match> getMatches(){
		return matches;
	}
	
	public String printMatches(){
		String retour="";
		for (Match m : matches)
		{
			retour+=(m.toString());
		}
		return retour;
	}

}
