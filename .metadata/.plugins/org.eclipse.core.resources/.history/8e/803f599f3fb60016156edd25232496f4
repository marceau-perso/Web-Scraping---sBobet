package MODEL;

import java.util.Vector;

public class Bet {
	private String match = null;
	private String market = null;
	private String odds1 = null;
	private String odds2 = null;
	private String oddsD = null;
	
	
	public Bet(String matchM, String marketM,String oddsH, String oddsA, String oddsD )
	{
		match = matchM;
		market = marketM;
		odds1 = oddsH;
		odds2 = oddsA;
		oddsD = oddsD;
		
	}
	public Bet(String matchM, String marketM,String oddsH, String oddsA )
	{
		match = matchM;
		market = marketM;
		odds1 = oddsH;
		odds2 = oddsA;
		
	}
	
	public String getMatch(){
		return match;
	}
	public String getMarket(){
		return market;
	}

}
